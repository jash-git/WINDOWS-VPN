eight=(parseInt(creativeFrame.frameElement.height)||parseInt(creativeFrame.frameElement.clientHeight));activeTop=frameTop+(frameHeight*0.5);lazyLoadTop=frameTop-150;if(lazyLoadTop<0){lazyLoadTop=0}frameBottom=frameTop+(frameHeight*1);activebottom=frameBottom-(frameHeight*0.5)}}else{console.log("reSetEventSize err")}}function initActiveView(){if(frameType!==null&&frameType!="cross Domain frame"){reSetEventSize();if(typeof(p)!="undefined"){try{topFrame.addEventListener("scroll",function(){if(typeof(setUpAvTrack)!="undefined"&&setUpAvTrack==true&&isActive===false){scrollEvent()}});scrollEvent()}catch(e){if(typeof(setUpAvTrack)!="undefined"&&setUpAvTrack==true){sendErLog(3,e)}}}}else{if(typeof(setUpAvTrack)!="undefined"&&setUpAvTrack==true){sendLog(0)}}}(function(){if(typeof(isLazyLoadActive)=="undefined"){var isLazyLoadActive=false}setTimeout(function(){try{getParent(self);if(zoneLazyLoad==0){setTimeout(function(){if(typeof(winBider)!="undefined"){cfLazyload()}},500)}if(frameType!==null&&frameType!="cross Domain frame"){var pageFrame=topFrame.document.getElementsByTagName("iframe");
